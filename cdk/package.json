{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "cdk": "cdk",
    "bundle": "cd lambda/nasa-fetch && yarn && zip -r bundle.zip .",
